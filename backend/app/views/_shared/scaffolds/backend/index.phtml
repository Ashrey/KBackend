{% extends 'default.phtml'%}
{%block title%}{{title}}{%endblock%}

{%block content%}
	{% if show_bar %}
		{% inline '_shared/scaffold_top.phtml'%}
	{%endif%}
	{%exec View::content %}
	{% autoescape off %}
		{%block result%}
			{%exec result->useCRUD %}
			{{result}}
		{%endblock%}
	{% endautoescape %}
{%endblock%}