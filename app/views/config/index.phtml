{% extends 'default.phtml'%}
{%block title%}Configurando{%endblock%}
{%block content%}
    <div class="offset-3 col-6" >
        {%exec Flash::content%}
        <ul>
        {%for key, val in values%}
        {%buffer url%}config/section/{{key}}{%endbuffer%}
        <li>{%link url, key %}</li>
        {%endfor%}
    </ul>
    </div>
</div>
{%endblock%}